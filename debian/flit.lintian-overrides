# Flist is a python development tools, so this is appropriate.
flit binary: application-in-library-section python usr/bin/flit

# These are for flit templates and tests, lintian is (understandably) wrong.
flit binary: script-not-executable usr/lib/python3/dist-packages/flit_core/tests/samples/package1/data_dir/foo.sh
flit binary: extra-license-file usr/lib/python3/dist-packages/flit/license_templates/gpl3
flit binary: extra-license-file usr/lib/python3/dist-packages/flit_core/tests/samples/pep517/LICENSE
flit binary: package-contains-documentation-outside-usr-share-doc usr/lib/python3/dist-packages/flit_core/tests/samples/pep517/LICENSE
flit binary: package-contains-documentation-outside-usr-share-doc usr/lib/python3/dist-packages/flit_core/tests/samples/pep517/README.rst
